.indexCard
    width:100%;
    height:80px;
    box-shadow: 1px 1px 3px #e8e8e8;
    border-radius:5px;
    margin-top: 10px;
    position:relative;
    padding:10px 0 10px 25px
    overflow hidden
    h2
        line-height:25px;
        color:#fff;
        font-weight: bold
        margin-top:1px
    h5
        color:#fff
    &.one 
        background:#967adc;
        .qq
            color:#cbbdee
            opacity 0.5
            position: absolute
            left:0
            top:0
    &.two
        background:#70ca63;
        .wechat
            color:#b8e5b1
            opacity 0.5
            position: absolute
            left:0
            top:0
    &.three
        background:#e9573f;
        .apple
            color:#f4ab9f
            opacity 0.5
            position: absolute
            left:0
            top:0
    &.four
        background:#3bafda;
        .github
            color:#9dd7ed
            opacity .5
